<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="874.0" prefWidth="1228.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DispositivosController">
   <children>
      <Label text="Dispositivos">
         <font>
            <Font size="24.0" />
         </font></Label>
      <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Button fx:id="btnNuevo" mnemonicParsing="false" onAction="#btnNuevoAction" text="Nuevo" />
            <Button fx:id="btnEliminar" mnemonicParsing="false" onAction="#btnEliminarAction" text="Eliminar" />
         </children></HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Label text="Buscar:" />
            <TextField fx:id="txtBuscar" prefHeight="25.0" prefWidth="200.0" promptText="Escribe para buscar..." />
            <Button fx:id="btnBuscar" mnemonicParsing="false" onAction="#btnBuscarAction" text="Buscar" />
         </children></HBox>
      <TableView fx:id="tablaDisp" prefHeight="665.0" prefWidth="1228.0">
        <columns>
          <TableColumn fx:id="colCodigo" prefWidth="75.0" text="Código" />
          <TableColumn fx:id="colNombre" prefWidth="74.0" text="Nombre" />
            <TableColumn fx:id="colModelo" minWidth="0.0" prefWidth="76.0" text="Modelo" />
            <TableColumn fx:id="colNumSerie" prefWidth="75.0" text="Num_serie" />
            <TableColumn fx:id="colNumEti" prefWidth="75.0" text="Num_etiq" />
            <TableColumn fx:id="colMac" prefWidth="75.0" text="Mac" />
            <TableColumn fx:id="colImei" prefWidth="75.0" text="Imei" />
            <TableColumn fx:id="colAlumno" prefWidth="75.0" text="Alumno" />
            <TableColumn fx:id="colCurso" prefWidth="75.0" text="Curso" />
            <TableColumn fx:id="colFechaAdqui" prefWidth="116.0" text="Fecha_adquisición" />
            <TableColumn fx:id="colProveedor" prefWidth="94.0" text="Proveedor" />
        </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
